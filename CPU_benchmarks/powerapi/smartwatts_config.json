{
  "verbose": true,
  "stream": true,
  "input": {
      "puller": {
      "model": "HWPCReport",
      "type": "mongodb",
      "uri": "mongodb://mongodb",
      "db": "db_sensor",
      "collection": "report_0"
    }
  },
  "output": {
      "pusher_power": {
      "type": "mongodb",
      "uri": "mongodb://mongodb",
      "db": "smartwatts",
      "collection": "power"
    }
  },
  "cpu-frequency-base": 2200,
  "cpu-frequency-min": 1000,
  "cpu-frequency-max": 3900,
  "cpu-error-threshold": 2.0,
  "disable-dram-formula": false,
  "sensor-report-sampling-interval": 1000
}
